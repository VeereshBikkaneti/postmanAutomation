	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"const postAuth = {",
					"    url: 'https://restful-booker.herokuapp.com/auth',",
					"    method: 'POST',",
					"    header: {",
					"        'Content-Type': 'application/json'",
					"    },",
					"    body: {",
					"        mode: 'raw',",
					"        raw: JSON.stringify({",
					"            \"username\": \"admin\",",
					"            \"password\": \"password123\"",
					"        })",
					"    }",
					"};",
					"pm.sendRequest(postAuth, (error, response) => {",
					"    var data = response.json();",
					"    var responseString = JSON.stringify(data);",
					"    var responseObj = JSON.parse(responseString);",
					"    pm.collectionVariables.set(\"auth\", \"token=\" + responseObj.token);",
					"    // console.log(pm.collectionVariables.get(\"auth\"));",
					"",
					"",
					"    let getBooking = \"https://restful-booker.herokuapp.com/booking/\"",
					"",
					"    pm.sendRequest(getBooking, function (err, response) {",
					"        var data = response.json();",
					"        var responseString = JSON.stringify(data);",
					"        var responseObj = JSON.parse(responseString);",
					"        let random = _.random(responseObj.length - 1);",
					"        pm.variables.set(\"ID\", responseObj[random].bookingid);",
					"    });",
					"",
					"});",
					"",
					"",
					"",
					"",
					""
				]
			}
